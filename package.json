{
  "name": "jixo",
  "version": "0.8.0",
  "description": "The AI-Driven Development Foundation for Automated Software Engineering",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "pub": "pnpm publish --access public --no-git-checks -r",
    "dev": "tsc --build --watch",
    "build": "rolldown -c ./rolldown.config.mts",
    "build2": "esbuild ./index.ts --format=cjs --outfile=./bundle/index.cjs --platform=node",
    "files-to-prompt": "node ./scripts/files-to-prompt.ts",
    "jixo": "node bundle/index.cjs"
  },
  "bin": "./bundle/index.cjs",
  "exports": {
    ".": {
      "default": "./bundle/index.cjs"
    }
  },
  "files": [
    "!scripts",
    "!packages"
  ],
  "keywords": [],
  "author": "Gaubee",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gaubee/jixo.git"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "@jixo/cli": "workspace:^"
  },
  "devDependencies": {
    "@gaubee/node": "^0.2.1",
    "@gaubee/nodekit": "^0.8.2",
    "@gaubee/util": "^0.32.1",
    "@parcel/watcher": "^2.5.1",
    "@std/cli": "jsr:^1.0.17",
    "@std/fmt": "jsr:^1.0.8",
    "@types/node": "^22.15.30",
    "esbuild": "^0.25.5",
    "import-meta-ponyfill": "^3.2.1",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rolldown": "1.0.0-beta.11",
    "typescript": "^5.8.3"
  }
}
