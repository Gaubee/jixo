{
  "name": "@jixo/cli",
  "version": "0.23.4",
  "type": "module",
  "bin": {
    "jixo": "./bundle/index.js"
  },
  "files": [
    "assets",
    "bundle",
    "dist"
  ],
  "scripts": {
    "prepack": "node scripts/prepack.ts",
    "postpack": "node scripts/postpack.ts",
    "build": "pnpm run \"/^b\\:.*/\"",
    "b:ts": "tsc --build",
    "b:bundle": "tsdown",
    "gen-prompts": "node ./scripts/gen-prompts.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./bundle/index.js"
    },
    "./dev": "./dist/index.js"
  },
  "dependencies": {
    "@parcel/watcher": "^2.5.1"
  },
  "peerDependencies": {
    "@jixo/dev": "workspace:^",
    "import-meta-ponyfill": "^3.2.2",
    "ts-pattern": "^5.7.1",
    "tslib": "^2.8.1",
    "yargs": "^18.0.0",
    "zod": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "@jixo/dev": {
      "optional": true
    },
    "import-meta-ponyfill": {
      "optional": true
    },
    "ts-pattern": {
      "optional": true
    },
    "tslib": {
      "optional": true
    },
    "yargs": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jixo/dev": "workspace:^",
    "@types/debug": "^4.1.12",
    "@types/json-schema": "^7.0.15",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.1.0",
    "@types/semver": "^7.7.0",
    "@types/yargs": "^17.0.33",
    "import-meta-ponyfill": "^3.2.2",
    "ts-pattern": "^5.7.1",
    "tslib": "^2.8.1",
    "yargs": "^18.0.0",
    "zod": "^4.0.0"
  }
}
