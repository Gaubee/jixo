{
  "name": "@jixo/cli",
  "version": "4.0.0",
  "type": "module",
  "bin": {
    "jixo": "./bundle/index.js"
  },
  "files": [
    "assets",
    "bundle",
    "dist"
  ],
  "scripts": {
    "prepack": "node scripts/prepack.ts",
    "postpack": "node scripts/postpack.ts",
    "build": "pnpm run \"/^b\\:.*/\"",
    "b:ts": "tsc --build",
    "b:bundle": "tsdown",
    "gen-prompts": "node ./scripts/gen-prompts.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./bundle/index.js"
    },
    "./dev": "./dist/index.js"
  },
  "dependencies": {
    "@parcel/watcher": "catalog:",
    "node-pty": "catalog:"
  },
  "peerDependencies": {
    "@jixo/dev": "workspace:^",
    "import-meta-ponyfill": "catalog:",
    "ts-pattern": "catalog:",
    "tslib": "catalog:",
    "yargs": "catalog:",
    "zod": "catalog:"
  },
  "peerDependenciesMeta": {
    "@jixo/dev": {
      "optional": true
    },
    "import-meta-ponyfill": {
      "optional": true
    },
    "ts-pattern": {
      "optional": true
    },
    "tslib": {
      "optional": true
    },
    "yargs": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@jixo/dev": "workspace:^",
    "@types/debug": "catalog:types",
    "@types/json-schema": "catalog:types",
    "@types/ms": "catalog:types",
    "@types/node": "catalog:types",
    "@types/semver": "catalog:types",
    "@types/yargs": "catalog:types",
    "import-meta-ponyfill": "catalog:",
    "ts-pattern": "catalog:",
    "tslib": "catalog:",
    "yargs": "catalog:",
    "zod": "catalog:"
  }
}
